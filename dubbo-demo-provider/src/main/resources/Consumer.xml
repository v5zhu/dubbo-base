<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
<!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
<!--<dubbo:application name="chop-app-consumer"/>-->

<dubbo:consumer timeout="${zookeeper.provider.timeout}"/>

<!-- 使用multicast广播注册中心暴露发现服务地址 -->
<dubbo:registry address="${zookeeper.consumber.address}"
                file="/data/dubbo/chop/chop-app-dubbo.cache"/>
<!--用户分组服务-->
<dubbo:reference id="developerService" interface="com.changhong.open.user.api.op.DeveloperService"
                 protocol="dubbo"/>
    <dubbo:reference id="userService" interface="com.changhong.open.user.api.op.UserService"
                     protocol="dubbo"/>
    <dubbo:reference id="noticeService" interface="com.changhong.open.user.api.op.NoticeService"
                     protocol="dubbo"/>

</beans>